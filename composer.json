{
    "name": "ajgarlag/psr15-router",
    "description": "Component to route PSR-7 requests through PSR-15 middlewares",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio J. GarcÃ­a Lagar",
            "email": "aj@garcialagar.es"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^7.2",
        "friendsofphp/php-cs-fixer": "^3.8",
        "nyholm/psr7": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Ajgarlag\\Psr15\\Router\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Ajgarlag\\Psr15\\Router\\": "./spec"
        }
    },
    "scripts": {
        "lint": "@php vendor/bin/php-cs-fixer fix --dry-run --ansi -v",
        "fix-cs": "@php vendor/bin/php-cs-fixer fix",
        "test": "@php vendor/bin/phpspec run"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5.x-dev"
        }
    }
}
